{
    "version": 2,
    "builds": [
        {
            "src": "/(.*)",
            "use": "@vercel/next"
        },
        {
            "src": "/api/(.*)",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}